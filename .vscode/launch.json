{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug import.ts",
      "runtimeArgs": [
        "--loader",
        "ts-node/esm"
      ],
      "args": [
        "${workspaceFolder}/src/import.ts"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_OPTIONS": "--no-warnings"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug eqmon.ts",
      "runtimeArgs": [
        "--loader",
        "ts-node/esm"
      ],
      "args": [
        "${workspaceFolder}/src/eqmon.ts"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_OPTIONS": "--no-warnings"
      }
    }
  ]
}
